var measurement = 'cpu'

var crit lambda

var log_file = '/tmp/alerts.log'

var outdb = 'mydb'

var alert_measurement = 'kapactor_alerts'

stream
    |from()
        .measurement(measurement)
    |alert()
         .crit(crit)
         .log(log_file)
    |influxDBOut()
         .database(outdb)
         .measurement(alert_measurement)
         .tag('kapacitor', 'true')
         .tag('version', '0.2')